<div class="col-8">
  <div class="container">
    <div class="row">
      <h1>Favorites' lists </h1>
      <div class="cards mb-5">
        <% @lists.each do |list| %>
          <%= render "list_card", list: list %>
        <% end %>
      </div>
    </div>
    <div class="row">
      <%= render "list_form" %>
    </div>
  </div>
</div>
